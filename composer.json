{
    "name": "olivion/rest-gen",
    "description": "RestFull Generating by Migration files",
    "license": "MIT",
    "authors": [
        {
            "name": "aladar",
            "email": "dev@olivion.tech",
            "homepage": "https://olivion.tech"
        }
    ],
    "homepage": "https://github.com/olivion/rest-gen",
    "keywords": ["Laravel", "RestGen"],
    "require": {
        "doctrine/dbal": "^3.6",
        "zircote/swagger-php": "^4.7",
        "flipbox/lumen-generator": "^9.2.0",
        "awobaz/compoships": "^2.2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Olivion\\RestGen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olivion\\RestGen\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Olivion\\RestGen\\RestGenServiceProvider"
            ],
            "aliases": {
                "RestGen": "Olivion\\RestGen\\Facades\\RestGen"
            }
        }
    }
}
